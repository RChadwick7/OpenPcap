
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option txpower '18'
	option hwmode '11g'
	option channel '1'

config wifi-iface 'sta'
	option device 'radio0'
	option network 'wwan'
	option mode 'sta'
	option ifname 'wlan-sta'
	option ssid 'xxx'
	option bssid 'F8:18:97:1F:F5:CE'
	option encryption 'psk2'
	option key 'xxx'
	option wds '0'
	option gl_bssid 'F8:18:97:1F:F5:CE'
	option channel '1'
	option disabled '0'

